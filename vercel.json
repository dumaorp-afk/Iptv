
{
  "version": 2,
  "builds": [
    {
      "src": "api/player_api.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "30mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/player_api",
      "dest": "api/player_api.py"
    }
  ]
}
